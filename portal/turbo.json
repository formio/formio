{
  "extends": [
    "//"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "src/**",
        "public/**",
        "package.json",
        "tsconfig.json",
        "turbo.json",
        "webpack.config.mjs"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "test/**",
        "vitest.config.ts"
      ],
      "outputs": []
    }
  }
}
